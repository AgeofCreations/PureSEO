Sub Sabaqa()
Dim array1() As String
Dim posStr As Long
Dim r As Integer
posStr = Cells(1, 1).End(xlDown).Row
r = posStr
Range("B1").value = posStr
For i2 = 1 To posStr
    var = ActiveCell.value
    array1 = Split(var, "#")
    arvalue = UBound(array1)
    
    For i = 0 To arvalue
        ActiveCell.value = array1(i)
        ActiveCell.Offset(1, 0).Select
        Selection.EntireRow.Insert
    Next i
    
    Selection.EntireRow.Delete
Next i2

Range("A2").Select
For i4 = 1 To r
    idval = ActiveCell.value
    ActiveCell.Offset(1, 0).Select
    For i3 = 1 To 12
    If ActiveCell.value < 2 Then
    
    ActiveCell.value = idval
    ActiveCell.Offset(1, 0).Select
    Else: Exit For
    ActiveCell.Offset(1, 0).Select
    End If
    Next i3
Next i4


End Sub
